import tkinter as tk
from system_info import SystemInfo

class MainWindow:
    def __init__(self, root):
        self.root = root
        self.root.title("Dashboard")
        self.system_info = SystemInfo()
        self.setup_ui()

    def setup_ui(self):
        # Display system information
        info_label = tk.Label(self.root, text=self.system_info.get_info(), font=('Arial', 12))
        info_label.pack(pady=20)

        # Refresh button to update the information
        refresh_button = tk.Button(self.root, text="Refresh", command=self.refresh_info)
        refresh_button.pack(pady=10)

    def refresh_info(self):
        # Update the information on the label
        info_label = self.root.pack_slaves()[0]
        info_label.config(text=self.system_info.get_info())

def create_app():
    root = tk.Tk()
    app = MainWindow(root)
    root.mainloop()

if __name__ == "__main__":
    create_app()
```

